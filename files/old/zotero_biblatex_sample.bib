  booktitle = {Socio-{{Environmental Dynamics}} along the {{Historical Silk Road}}},
  author = {Mächtle, Bertil and Hecht, Stefan and Manke, Nicola and Kromer, Bernd and Lindauer, Susanne and Li, Cheng-Sen and Li, Ying and Wang, Xiaofei and Bubenzer, Olaf},
