@incollection{VyazovDemographic2019,
  title = {Demographic {{Changes}}, {{Trade Routes}}, and the {{Formation}} of {{Anthropogenic Landscapes}} in the {{Middle Volga Region}} in the {{Past}} 2500 {{Years}}},
  booktitle = {Socio-{{Environmental Dynamics}} along the {{Historical Silk Road}}},
