}

@article{AliSinging2005,
  title = {Singing {{Samarra}} (861-956): {{Poetry}} and the {{Burgeoning}} of {{Historiography}} upon the {{Murder}} of al-{{Mutawakkil}}},
  shorttitle = {Singing {{Samarra}} (861-956)},
  author = {Ali, Samer Mahdy},
  date = {2005},
  journaltitle = {Journal of Arabic and Islamic Studies},
  pages = {1--23},
  issn = {0806-198X},
  doi = {10.5617/jais.4576},
  abstract = {Historiography on the patricide/regicide of the Caliph al-Mutawakkil (d.~861) developed from a stage of simple description to a burgeoning of~mytho-historical narrative. It would appear that what began as a palace~scandal—profaning to a putatively sacral community already torn by civil~war—developed into a redemptive tragedy with perennial appeal. In a~patronage society governed by loyalty to one’s patron or father, this transformation~should count as nothing less than conspicuous. This article~examines the role of a major Abbasid poet, al-Buḥturī (d. 897), in shaping~public perception by cultivating genuine sympathy for the Abbasids and~planting the seeds of questions that would be addressed in historical narratives.~In particular, I discuss the importance of literary salons or gatherings~as a social institution where poetry and historical narratives were recited~orally as a means of transmitting knowledge to future generations. These gatherings provide a likely forum where mythic questions of poetry could inspire narrative.},
  file = {/path/to/file/ZoteroBooks/AliS/Ali 2005 - Singing Samarra (861-956).pdf;/path/to/file/ZoteroApp/storage/SR3MA9V7/4576.html},
  langid = {english}
}

