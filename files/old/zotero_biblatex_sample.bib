@incollection{ZamanTransmitters2010,
  title = {Transmitters of Authority and Ideas across Cultural Boundaries, Eleventh to Eighteenth Centuries},
  booktitle = {The {{New Cambridge History}} of {{Islam}}: {{Volume}} 3: {{The Eastern Islamic World}}, {{Eleventh}} to {{Eighteenth Centuries}}},
  author = {Zaman, Muhammad},
